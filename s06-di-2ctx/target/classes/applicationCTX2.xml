<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	<bean id="student2" class="com.model.s06.Student">
		<constructor-arg index="0" value="고리" />  	
 	    <constructor-arg index="1" value="35살" />
		<constructor-arg>
			<list>
				<value>말아톤</value>
				<value>도박</value>
			</list>
		</constructor-arg> 	
		<property name="height" value="177" />
		<property name="weight" value="68" />		
	</bean>
	<bean id="studentInfo1" class="com.model.s05.StudentInfo">
		<property name="student" ref="student1" /> 		
	</bean>

</beans>
